%
% MS.sty
% Style file to set up title and signature pages for MS thesis.
% Do not edit this file unless there is an absolute need, and/or in the
% absence of knowledge of LaTeX style files.

%
% Title page
\def\titlepage{%
  \sloppy
  \pagenumbering{roman}
  \pagestyle{plain}
  \thispagestyle{empty}%
  \null\vskip.50in%

  \begin{center}
    \expandafter{\@title}
  \end{center}

  \vspace{\fill}
  \begin{center}
    A THESIS\\
    Submitted in partial fulfillment of the requirements for the degree of\\
    MASTER OF SCIENCE\\
    In \expandafter{\@program}
  \end{center}

  \begin{center}
    \uppercase\expandafter{\@university}\\
    \@date
  \end{center}

 \vspace{\fill}
\begin{center}
	By\\
	\expandafter{\@author} \\
	\expandafter{\@priordegree}
\end{center}

  \vfill
  \begin{center}
	  \begin{tabular}{cc}%
	  	\ifx \@coadvisor \@empty 
	  	Thesis Advisor:    & \hspace{0.10in}\textsl{\expandafter{\@primaryadvisor}}\\
	  	\else
	  	Thesis Co-advisor: & \hspace{0.10in}\textsl{\expandafter{\@primaryadvisor}}\\
	  	Thesis Co-advisor: & \hspace{0.10in}\textsl{\expandafter{\@coadvisor}}\\
	  	\fi
	  \end{tabular}%
  \end{center}

  \newpage
}


